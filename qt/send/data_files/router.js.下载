var router = angular.module('dc.router',["ngRoute",
	'dc.controller.life.lifeIndexCtrl',
	'dc.controller.canteen.canteenCtrl',
	'dc.controller.freeClassroom.freeClassroomCtrl',
	'dc.controller.cadreContact.cadreContactCtrl',
	'dc.controller.courseIntro.courseIntroCtrl',
	'dc.controller.courseSchedule.courseScheduleCtrl',
	'dc.controller.tutorQuery.tutorQueryCtrl',
	'dc.controller.myScore.myScoreCtrl',
	'dc.controller.courseTable.myCourseTableCtrl',
	'dc.controller.personQuery.personQueryCtrl',
	'dc.controller.personQuery.personDetailCtrl',
	'dc.controller.mobileIdentify.mobileIdentifyCtrl',
	'dc.controller.faceIdentify.faceIdentifyCtrl',
	'dc.controller.fileTrans.myFileTransCtrl',
	'dc.controller.certificatePhoto.certificatePhotoCtrl',
	'dc.controller.heds.hedsSvcQueryCtrl',
	'dc.controller.income.incomeCtrl',
	'dc.controller.abroadBiz.abroadBizCtrl',
	'dc.controller.abroadBiz.passportEditCtrl'
]);	
router.config(function($routeProvider, $locationProvider) {
	$locationProvider.html5Mode(false);
	$routeProvider.when('/life',{
    	 templateUrl: '/publicQuery/app/view/life/lifeIndex.html?_v=20240918',
    	 controller:'lifeIndexController'
     }).when('/canteen',{
    	 templateUrl: '/publicQuery/app/view/canteen/canteen.html?_v=20240918',
    	 controller:'canteenController'
     }).when('/freeClassroom',{
    	 templateUrl: '/publicQuery/app/view/freeClassroom/freeClassroom.html?_v=20240918',
    	 controller:'freeClassroomController'
     }).when('/courseIntro',{
    	 templateUrl: '/publicQuery/app/view/courseIntro/courseIntro.html?_v=20240918',
    	 controller:'courseIntroController'
     }).when('/courseSchedule',{
    	 templateUrl: '/publicQuery/app/view/courseSchedule/courseSchedule.html?_v=20240918',
    	 controller:'courseScheduleController'
     }).when('/tutorQuery',{
    	 templateUrl: '/publicQuery/app/view/tutorQuery/tutorQuery.html?_v=20240918',
    	 controller:'tutorQueryController'
     }).when('/cadreContact',{
    	 templateUrl: '/publicQuery/app/view/cadreContact/cadreContact.html?_v=20240918',
    	 controller:'cadreContactController'
     }).when('/myScore',{
    	 templateUrl: '/publicQuery/app/view/myScore/myScore.html?_v=20240918',
    	 controller:'myScoreController'
     }).when('/myCourseTable',{
    	 templateUrl: '/publicQuery/app/view/myCourseTable/myCourseTable.html?_v=20240918',
    	 controller:'myCourseTableController'
     }).when('/myFileTrans',{
    	 templateUrl: '/publicQuery/app/view/myFileTrans/myFileTrans.html?_v=20240918',
    	 controller:'myFileTransController'
     }).when('/personQuery',{
    	 templateUrl: '/publicQuery/app/view/personQuery/personQuery.html?_v=20240918',
    	 controller:'personQueryController'
     }).when('/personQuery/detail/:seqNo',{
    	 templateUrl: '/publicQuery/app/view/personQuery/personDetail.html?_v=20240918',
    	 controller:'personDetailController'
     }).when('/mobileIdentify',{
    	 templateUrl: '/publicQuery/app/view/mobileIdentify/mobileIdentify.html?_v=20240918',
    	 controller:'mobileIdentifyController'
     }).when('/faceIdentify',{
    	 templateUrl: '/publicQuery/app/view/faceIdentify/faceIdentify.html?_v=20240918',
    	 controller:'faceIdentifyController'
     }).when('/certificatePhoto/:stuType',{
    	 templateUrl: '/publicQuery/app/view/certificatePhoto/certificatePhoto.html?_v=20240918',
    	 controller:'certificatePhotoController'
     }).when('/pkuhall/ticket',{
    	 templateUrl: '/publicQuery/app/view/pkuhall/ticket.html?_v=20240918'
     }).when('/hedsSvcQuery',{
    	 templateUrl: '/publicQuery/app/view/heds/hedsSvcQuery.html?_v=20240918',
    	 controller:'hedsSvcQueryController'
     }).when('/income/:language',{
    	 templateUrl: '/publicQuery/app/view/income/incomes.html?_v=20240918',
    	 controller:'incomeController'
     }).when('/abroadBiz',{
    	 templateUrl: '/publicQuery/app/view/abroadBiz/abroadBiz.html?_v=20250408',
         controller:'abroadBizController'
     }).when('/abroadBiz/pspt',{
    	 templateUrl: '/publicQuery/app/view/abroadBiz/addPassport.html?_v=20250408',
         controller:'passportEditController'
     }).when('/abroadBiz/pspt/:psptNo',{
    	 templateUrl: '/publicQuery/app/view/abroadBiz/editPassport.html?_v=20250408',
         controller:'passportEditController'
     }).when('/403',{
    	 templateUrl: '/publicQuery/app/view/403.html?_v=20240918'
     }).otherwise({
         redirectTo:"/403"
     });
});