var myFileTransCtrl = angular.module('dc.controller.fileTrans.myFileTransCtrl', []);	
myFileTransCtrl.controller("myFileTransController",['$scope','$http',function($scope,$http){
	$scope.errorMsg = "";
		
	$scope.xh = "";
    $scope.xm = "";
    $scope.yb = "";
    $scope.dwlxr = "";
   	$scope.dwlxdh = "";
    $scope.dazjdw = "";
    $scope.dazjdz = "";
    $scope.yjbh = "";
		
	$scope.getFileTransInfo = function(){
		$http({
	    	method:'GET',
	        dataType: "json",
	        contentType:'application/json; charset=UTF-8',
	        url:'/publicQuery/ctrl/topic/myFileTrans/getFileTransInfo.do'
	    }).success(function(response){
	        if(true==response.success){
	        	$scope.errorMsg = null;
	        	$scope.xh = response.xh;
	 	       	$scope.xm = response.xm;
	 	        $scope.yb = response.yb;
	 	        $scope.dwlxr = response.dwlxr;
	 	        $scope.dwlxdh = response.dwlxdh;
	 	        $scope.dazjdw = response.dazjdw;
	 	        $scope.dazjdz = response.dazjdz;
	 	        $scope.yjbh = response.yjbh;
	        }else{
	        	$scope.errorMsg = response.message||response.msg;
	        }
	   }).error(function(){
	        $scope.errorMsg = "服务器没有响应";
	   });
	};
		
    //初始化函数
    var init = function() {
        $scope.getFileTransInfo();
    };
    init();		
}]);